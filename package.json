{
  "name": "ookami",
  "version": "1.0.0",
  "description": "Bot for discord multipurpose",
  "main": "core/index.js",
  "type": "commonjs",
  "scripts": {
    "test": "nodemon .",
    "start": "node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PintoGamer64/Ookami-Bot.git"
  },
  "keywords": [
    "Bot",
    "Discord",
    "Ookami",
    "PintoGamer",
    "NodeJs"
  ],
  "author": "PintoGamer64",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PintoGamer64/Ookami-Bot/issues"
  },
  "homepage": "https://github.com/PintoGamer64/Ookami-Bot#readme",
  "dependencies": {
    "discord.js": "^14.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "hispamemes": "^1.0.7",
    "image-to-colors": "^1.0.2"
  },
  "devDependencies": {
    "eslint": "^8.25.0",
    "nodemon": "^2.0.20"
  },
  "extends": "eslint:recommended",
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    "arrow-spacing": [
      "warn",
      {
        "before": true,
        "after": true
      }
    ],
    "brace-style": [
      "error",
      "stroustrup",
      {
        "allowSingleLine": true
      }
    ],
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "comma-spacing": "error",
    "comma-style": "error",
    "curly": [
      "error",
      "multi-line",
      "consistent"
    ],
    "dot-location": [
      "error",
      "property"
    ],
    "handle-callback-err": "off",
    "indent": [
      "error",
      "tab"
    ],
    "keyword-spacing": "error",
    "max-nested-callbacks": [
      "error",
      {
        "max": 4
      }
    ],
    "max-statements-per-line": [
      "error",
      {
        "max": 2
      }
    ],
    "no-console": "off",
    "no-empty-function": "error",
    "no-floating-decimal": "error",
    "no-inline-comments": "error",
    "no-lonely-if": "error",
    "no-multi-spaces": "error",
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 2,
        "maxEOF": 1,
        "maxBOF": 0
      }
    ],
    "no-shadow": [
      "error",
      {
        "allow": [
          "err",
          "resolve",
          "reject"
        ]
      }
    ],
    "no-trailing-spaces": [
      "error"
    ],
    "no-var": "error",
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "prefer-const": "error",
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "space-before-blocks": "error",
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "never",
        "named": "never",
        "asyncArrow": "always"
      }
    ],
    "space-in-parens": "error",
    "space-infix-ops": "error",
    "space-unary-ops": "error",
    "spaced-comment": "error",
    "yoda": "error"
  }
}
